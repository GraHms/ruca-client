{
  "expo": {
    "name": "ruca",
    "slug": "ruca",
    "version": "1.0.0",
    "sdkVersion": "54.0.0",
    "runtimeVersion": { "policy": "sdkVersion" },
    "orientation": "portrait",
    "scheme": "ruca",
    "userInterfaceStyle": "light",
    "splash": {
      "backgroundColor": "#0f766e"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "package": "mz.ruca.app"
    },
    "web": {
      "bundler": "metro",
      "output": "single-page"
    },
    "plugins": [
      "nativewind/babel"
    ],
    "extra": {
      "eas": {
        "projectId": "00000000-0000-0000-0000-000000000000"
      },
      "googleMapsApiKey": null
    }
  }
}
